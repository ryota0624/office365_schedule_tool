{
  "manifest_version": 2,
  "name": "outlookの馬鹿野郎！！！！",
  "version": "0.0.0",
  "permissions": [
    "*://*/*/",
    "webRequest",
    "tabs",
    "cookies"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }]
}
